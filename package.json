{
  "name": "steal-sass",
  "version": "1.0.2",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test:browser": "testee test/test.html --browsers firefox --reporter Spec",
    "test:node": "mocha test/node_test.js",
    "test": "npm run test:browser && npm run test:node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stealjs/steal-sass.git"
  },
  "author": "Bitovi",
  "license": "MIT",
  "dependencies": {
    "sass.js": "^0.9.4"
  },
  "bugs": {
    "url": "https://github.com/stealjs/steal-sass/issues"
  },
  "system": {
    "ext": {
      "scss": "steal-sass"
    },
    "map": {
      "$sass.js": "steal-sass/browsersass"
    },
    "envs": {
      "build": {
        "map": {
          "$sass.js": "steal-sass/nodesass"
        }
      },
      "build-development": {
        "map": {
          "$sass.js": "steal-sass/nodesass"
        }
      },
      "server-development": {
        "map": {
          "$sass.js": "steal-sass/nodesass"
        }
      },
      "server-production": {
        "map": {
          "$sass.js": "steal-sass/nodesass"
        }
      }
    },
    "npmIgnore": [
      "testee",
      "mocha"
    ]
  },
  "devDependencies": {
    "funcunit": "^3.0.0",
    "mocha": "^2.3.4",
    "steal": "^0.16.0",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.16.0",
    "testee": "^0.2.2"
  },
  "homepage": "https://github.com/stealjs/steal-sass"
}
