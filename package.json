{
  "name": "steal-sass",
  "version": "1.0.4",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test:browser": "testee test/test.html --browsers firefox --reporter Spec",
    "test:node": "mocha test/node_test.js",
    "test": "npm run test:browser && npm run test:node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stealjs/steal-sass.git"
  },
  "author": "Bitovi",
  "license": "MIT",
  "dependencies": {
    "sass.js": "^0.9.4"
  },
  "bugs": {
    "url": "https://github.com/stealjs/steal-sass/issues"
  },
  "system": {
    "ext": {
      "scss": "steal-sass"
    },
    "plugins": ["steal-css"],
    "map": {
      "$sass": "steal-sass/browsersass"
    },
    "envs": {
      "build": {
        "map": {
          "$sass": "steal-sass/nodesass"
        }
      },
      "build-development": {
        "map": {
          "$sass": "steal-sass/nodesass"
        }
      },
      "server-development": {
        "map": {
          "$sass": "steal-sass/nodesass"
        }
      },
      "server-production": {
        "map": {
          "$sass": "steal-sass/nodesass"
        }
      }
    },
    "npmIgnore": [
      "testee",
      "mocha"
    ]
  },
  "devDependencies": {
    "funcunit": "^3.0.0",
    "mocha": "^2.3.4",
    "steal": "^1.3.1",
    "steal-css": "^1.2.1",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.1"
  },
  "homepage": "https://github.com/stealjs/steal-sass"
}
